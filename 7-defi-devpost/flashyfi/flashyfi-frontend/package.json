    "@types/lodash": "^4.14.191",
    "lodash": "^4.17.21",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
